@model Asistencia.Models.ViewModels.CourseDetailsViewModel
<div class="container">
    <div class="secondary-menu">
        <ul class="nav nav-tabs" id="attendancesManager" role="tablist">
            <li class="nav-item" role="presentation" >
                <button class="nav-link active" id="AttendnaceRegister-tab" data-bs-toggle="tab" data-bs-target="#attendance-register" type="button" role="tab" aria-controls="register" aria-selected="true">
                    <i class="fas fa-check-double"></i> Registrar
                </button>
            </li>
            <li class="nav-item" role="presentation" >
                <button class="nav-link" id="AttendnaceHistory-tab" data-bs-toggle="tab" data-bs-target="#attendance-history" type="button" role="tab" aria-controls="history" aria-selected="false">
                    <i class="fas fa-list-check"></i> Historial
                </button>
            </li>
        </ul>
    </div>
    <div class="tab-content" id="AttendanceMenuContet">
        <!-- Register Attendance Tab -->
        <div class="tab-pane fade show active" id="attendance-register" role="tabpanel" aria-labelledby="atendance-register-tab" >
            <partial name="_AttendancesRegisterTab" />
        
        </div> <!-- Register Attendance Tab End-->
         <!-- History Attendance Tab -->
        <div class="tab-pane fade" id="attendance-history" role="tabpanel" aria-labelledby="attendance-hisoty-tab">
            <partial name="_AttendancesSessions" />
        </div> <!-- History Attendance Tab End-->
    </div> <!-- AttendanceMenuContent End-->
</div>
