<div class="container">
<table class="table table-responsive table-striped table-sm">
    <thead>
            <tr>
                <th class="col">Código</td>
                <th class="col"> Nombre del Aula</td>
                <th class="col">Capacidad</td>
                <th class="col">Ubicación</td>
                <th class="col">Estado</td>
                <th class="col">Actions</td>
            </tr>
    </thead>
    <tbody class="table-hover">
        
        @foreach( var classrooms in @Model){
        <tr>
            <td>@classrooms.ClassroomId</td>
            <td>@classrooms.ClassroomName</td>
            <td>@classrooms.Capacity</td>
            <td>@classrooms.Location</td>
            <td>Disponble</td>
            <td>
                <!--Edit Classroom-->
                <a href="javascript:void(0)" class="btn btn-warning btn-sm btn-edit" data-id="@classrooms.ClassroomId"> <i class="fas fa-edit"></i></a>
                <button class="btn text-primary" ><i class="fas fa-calendar "></i> </button>
            </td>
        </tr>
        }
    </tbody>
</table>
</div>

